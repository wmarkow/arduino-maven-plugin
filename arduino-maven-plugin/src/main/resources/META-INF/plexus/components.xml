<?xml version="1.0"?>
<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>arduinolib</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <initialize>
                                com.github.wmarkow.amp:arduino-maven-plugin:fetch-dependencies
                            </initialize>
                            <generate-sources>
                                com.github.wmarkow.amp:arduino-maven-plugin:unpack-dependencies
                            </generate-sources>
                            <compile>
                                com.github.wmarkow.amp:arduino-maven-plugin:compile
                            </compile>
                            <package>
                                org.apache.maven.plugins:maven-assembly-plugin:single,
                                com.github.wmarkow.amp:arduino-maven-plugin:package
                            </package>
                            <install>
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>arduinoelf</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <initialize>
                                com.github.wmarkow.amp:arduino-maven-plugin:fetch-dependencies
                            </initialize>
                            <generate-sources>
                                com.github.wmarkow.amp:arduino-maven-plugin:unpack-dependencies
                            </generate-sources>
                            <compile>
                                com.github.wmarkow.amp:arduino-maven-plugin:compile,
                                com.github.wmarkow.amp:arduino-maven-plugin:link,
                                com.github.wmarkow.amp:arduino-maven-plugin:obj-dump
                            </compile>
                            <package>
                                com.github.wmarkow.amp:arduino-maven-plugin:package
                            </package>
                            <install>
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
